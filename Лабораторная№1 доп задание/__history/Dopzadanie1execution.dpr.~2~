program Dopzadanie1execution; uses System.SysUtils; var N:integer; function GetNOD(a:integer): Integer; begin var index:integer; index:=2; var check:boolean; check:=True; while check and (index<=a) do begin if (a mod index=0) and (N mod index=0) then begin Result:=index; check:=false; end; index:=index+1; end; if (check) and (N mod a<>0) then Result:=1; end; begin write('Введите N:'); Readln(N); Write('Натуральные числа меньшие N и взаимно простые с N:'); var b:integer; var i:integer; for i := 2 to N do begin if GetNOD(i)=1 then begin write(i.ToString()+' '); end; end; readln; end.




